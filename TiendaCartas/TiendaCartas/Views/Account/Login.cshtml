@using TiendaCartas.Models
@model LoginViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Iniciar sesión";
}

<h2>@ViewBag.Title.</h2>
<div class="iniciarSesion">
    <p style="font-size:15pt;">Ingrese Correo electronico:</p>
    <input type="email" />
    <p style="font-size:15pt;">Ingrese Contraseña:</p>
    <input type="password" />  
    <br />
    <br />
    <input type="button" class="btn-default" value="Iniciar Sesion"/>

</div>